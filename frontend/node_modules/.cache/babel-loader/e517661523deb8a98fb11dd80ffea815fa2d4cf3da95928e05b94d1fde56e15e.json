option v-for=\"c in categorias\" :key=\"c.id\" :value=\"c.id\">{{ c.nombre }}</option>\r\n          </select>\r\n        </div>\r\n\r\n        <div class=\"col-md-3 mb-3\">\r\n          <label class=\"form-label\">Presentación</label>\r\n          <select v-model=\"form.presentacion_id\" class=\"form-control\" required>\r\n            <option value=\"\" disabled>Seleccione</option>\r\n            <option v-for=\"pr in presentaciones\" :key=\"pr.id\" :value=\"pr.id\">{{ pr.nombre }}</option>\r\n          </select>\r\n        </div>\r\n\r\n        <div class=\"col-md-2 mb-3 d-flex align-items-center\">\r\n          <input v-model=\"form.activo\" type=\"checkbox\" class=\"form-check-input me-2\" :true-value=\"true\" :false-value=\"false\" />\r\n          <label class=\"form-check-label\">Activo</label>\r\n        </div>\r\n      </div>\r\n\r\n      <button :disabled=\"loading\" type=\"submit\" class=\"btn btn-primary\">\r\n        {{ editando ? 'Actualizar' : 'Agregar' }} Producto\r\n      </button>\r\n      <button v-if=\"editando\" :disabled=\"loading\" type=\"button\" class=\"btn btn-secondary ms-2\" @click=\"cancelarEdicion\">\r\n        Cancelar\r\n      </button>\r\n    </form>\r\n\r\n    <!-- Búsqueda -->\r\n    <div class=\"mb-3\">\r\n      <input\r\n        v-model=\"search\"\r\n        type=\"text\"\r\n        class=\"form-control\"\r\n        placeholder=\"Buscar por nombre...\"\r\n        @input=\"onSearchInput\"\r\n      />\r\n    </div>\r\n\r\n    <!-- Tabla -->\r\n    <table class=\"table table-bordered table-striped\">\r\n      <thead>\r\n        <tr>\r\n          <th style=\"width:80px;\">ID</th>\r\n          <th>Nombre</th>\r\n          <th style=\"width:120px;\">Precio</th>\r\n          <th>Categoría</th>\r\n          <th>Presentación</th>\r\n          <th style=\"width:90px;\">Activo</th>\r\n          <th style=\"width:180px;\">Acciones</th>\r\n        </tr>\r\n      </thead>\r\n      <tbody>\r\n        <tr v-for=\"p in productos\" :key=\"p.id\">\r\n          <td>{{ p.id }}</td>\r\n          <td>{{ p.nombre }}</td>\r\n          <td>{{ formatearPrecio(p.precio) }}</td>\r\n          <td>{{ p.categoria }}</td>\r\n          <td>{{ p.presentacion }}</td>\r\n          <td>\r\n            <span class=\"badge\" :class=\"p.activo ? 'bg-success' : 'bg-danger'\">\r\n              {{ p.activo ? 'Sí' : 'No' }}\r\n            </span>\r\n          </td>\r\n          <td>\r\n            <button class=\"btn btn-warning btn-sm me-2\" @click=\"editarProducto(p)\">Editar</button>\r\n            <button class=\"btn btn-danger btn-sm\" @click=\"confirmarEliminar(p.id)\">Eliminar</button>\r\n          </td>\r\n        </tr>\r\n        <tr v-if=\"!loading && productos.length === 0\">\r\n          <td colspan=\"7\" class=\"text-center\">Sin resultados</td>\r\n        </tr>\r\n        <tr v-if=\"loading\">\r\n          <td colspan=\"7\" class=\"text-center\">Cargando...</td>\r\n        </tr>\r\n      </tbody>\r\n    </table>\r\n\r\n    <!-- Paginación -->\r\n    <nav>\r\n      <ul class=\"pagination\">\r\n        <li class=\"page-item\" :class=\"{ disabled: page === 1 || loading }\">\r\n          <button class=\"page-link\" @click=\"cambiarPagina(page - 1)\" :disabled=\"page === 1 || loading\">Anterior</button>\r\n        </li>\r\n        <li class=\"page-item\">\r\n          <span class=\"page-link\">{{ page }}</span>\r\n        </li>\r\n        <li class=\"page-item\">\r\n          <button class=\"page-link\" @click=\"cambiarPagina(page + 1)\" :disabled=\"loading\">Siguiente</button>\r\n        </li>\r\n      </ul>\r\n    </nav>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Swal from \"sweetalert2\";\r\nimport http from \"@/api/http\"; // axios con interceptores (access + refresh)\r\n\r\nexport default {\r\n  name: \"ProductoView\",\r\n  data() {\r\n    return {\r\n      productos: [],\r\n      categorias: [],\r\n      presentaciones: [],\r\n      form: { id: null, nombre: \"\", precio: null, categoria_id: \"\", presentacion_id: \"\", activo: true },\r\n      editando: false,\r\n      page: 1,\r\n      per_page: 10,\r\n      search: \"\",\r\n      loading: false,\r\n      searchTimer: null,\r\n    };\r\n  },\r\n  mounted() {\r\n    // Cargar cat/pres en paralelo\r\n    this.cargarCategorias();\r\n    this.cargarPresentaciones();\r\n    this.cargarProductos();\r\n  },\r\n  methods: {\r\n    formatearPrecio(v) {\r\n      const n = Number(v);\r\n      if (Number.isNaN(n)) return \"-\";\r\n      return n.toFixed(2);\r\n    },\r\n\r\n    // --------- Cargas ----------\r\n    async cargarCategorias() {\r\n      try {\r\n        const { data } = await http.get(\"/categorias/\", { params: { page: 1, per_page: 999 } });\r\n        this.categorias = Array.isArray(data?.data) ? data.data : (Array.isArray(data) ? data : []);\r\n      } catch (e) {\r\n        console.error(e);\r\n        Swal.fire(\"Error\", e?.response?.data?.error || \"Error al cargar categorías\", \"error\");\r\n      }\r\n    },\r\n\r\n    async cargarPresentaciones() {\r\n      try {\r\n        const { data } = await http.get(\"/presentaciones/\", { params: { page: 1, per_page: 999 } });\r\n        this.presentaciones = Array.isArray(data?.data) ? data.data : (Array.isArray(data) ? data : []);\r\n      } catch (e) {\r\n        console.error(e);\r\n        Swal.fire(\"Error\", e?.response?.data?.error || \"Error al cargar presentaciones\", \"error\");\r\n      }\r\n    },\r\n\r\n    async cargarProductos() {\r\n      this.loading = true;\r\n      try {\r\n        const params = { page: this.page, per_page: this.per_page, search: this.search };\r\n        const { data } = await http.get(\"/productos/\", { params });\r\n        // backend: { total, page, per_page, items: [] }\r\n        this.productos = Array.isArray(data?.items) ? data.items : [];\r\n      } catch (e) {\r\n        console.error(e);\r\n        Swal.fire(\"Error\", e?.response?.data?.error || \"Error al cargar productos\", \"error\");\r\n      } finally {\r\n        this.loading = false;\r\n      }\r\n    },\r\n\r\n    onSearchInput() {\r\n      clearTimeout(this.searchTimer);\r\n      this.searchTimer = setTimeout(() => {\r\n        this.page = 1;\r\n        this.cargarProductos();\r\n      }, 350);\r\n    },\r\n\r\n    // --------- Guardar ----------\r\n    async confirmarGuardar() {\r\n      const result = await Swal.fire({\r\n        title: \"¿Está seguro que desea guardar la información ingresada?\",\r\n        icon: \"question\",\r\n        showCancelButton: true,\r\n        confirmButtonText: \"Sí, guardar\",\r\n        cancelButtonText: \"Cancelar\",\r\n      });\r\n      if (result.isConfirmed) this.guardarProducto();\r\n    },\r\n\r\n    async guardarProducto() {\r\n      // Validaciones básicas\r\n      if (!this.form.nombre?.trim()) return Swal.fire(\"Validación\", \"El nombre es requerido\", \"warning\");\r\n      if (this.form.precio === null || this.form.precio === \"\" || Number(this.form.precio) < 0) {\r\n        return Swal.fire(\"Validación\", \"El precio es requerido y debe ser mayor o igual a 0\", \"warning\");\r\n      }\r\n      if (!this.form.categoria_id) return Swal.fire(\"Validación\", \"Seleccione una categoría\", \"warning\");\r\n      if (!this.form.presentacion_id) return Swal.fire(\"Validación\", \"Seleccione una presentación\", \"warning\");\r\n\r\n      this.loading = true;\r\n      try {\r\n        const url = this.editando ? `/productos/${this.form.id}` : `/productos/`;\r\n        const method = this.editando ? \"put\" : \"post\";\r\n        const payload = {\r\n          nombre: this.form.nombre?.trim(),\r\n          precio: Number(this.form.precio),\r\n          categoria_id: this.form.categoria_id,\r\n          presentacion_id: this.form.presentacion_id,\r\n          activo: !!this.form.activo,\r\n        };\r\n\r\n        const { status, data } = await http[method](url, payload);\r\n        if (!String(status).startsWith(\"2\")) {\r\n          throw new Error(data?.error || \"Error al guardar el producto\");\r\n        }\r\n\r\n        this.resetForm();\r\n        this.editando = false;\r\n        await this.cargarProductos();\r\n        Swal.fire(\"Éxito\", \"Producto guardado correctamente\", \"success\");\r\n      } catch (e) {\r\n        console.error(e);\r\n        Swal.fire(\"Error\", e?.response?.data?.error || \"Error al guardar el producto\", \"error\");\r\n      } finally {\r\n        this.loading = false;\r\n      }\r\n    },\r\n\r\n    // --------- Editar ----------\r\n    async editarProducto(p) {\r\n      const result = await Swal.fire({\r\n        title: \"¿Editar producto?\",\r\n        text: `¿Deseas editar el producto \"${p.nombre}\"?`,\r\n        icon: \"question\",\r\n        showCancelButton: true,\r\n        confirmButtonText: \"Sí, editar\",\r\n        cancelButtonText: \"Cancelar\",\r\n      });\r\n      if (!result.isConfirmed) return;\r\n\r\n      // Mapear a form (si backend entrega nombres en vez de IDs, el usuario elegirá nuevos IDs)\r\n      this.form = {\r\n        id: p.id,\r\n        nombre: p.nombre,\r\n        precio: p.precio,\r\n        // Estos deben ser IDs. Si tu tabla devuelve nombres, déjalos vacíos para que el usuario escoja:\r\n        categoria_id: this.buscarId(this.categorias, p.categoria),\r\n        presentacion_id: this.buscarId(this.presentaciones, p.presentacion),\r\n        activo: !!p.activo,\r\n      };\r\n      this.editando = true;\r\n      window.scrollTo({ top: 0, behavior: \"smooth\" });\r\n    },\r\n\r\n    buscarId(lista, nombre) {\r\n      if (!nombre) return \"\";\r\n      const found = (lista || []).find((x) => (x?.nombre || \"\").toLowerCase() === String(nombre).toLowerCase());\r\n      return found?.id ?? \"\";\r\n    },\r\n\r\n    cancelarEdicion() {\r\n      this.resetForm();\r\n      this.editando = false;\r\n    },\r\n\r\n    // --------- Eliminar ----------\r\n    async confirmarEliminar(id) {\r\n      const result = await Swal.fire({\r\n        title: \"¿Estás seguro?\",\r\n  
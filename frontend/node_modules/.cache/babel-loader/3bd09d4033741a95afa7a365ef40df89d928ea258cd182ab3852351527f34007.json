{"ast":null,"code":"import Swal from \"sweetalert2\";\nimport http from \"@/api/http\"; // axios con interceptores (access + refresh)\n\nexport default {\n  name: \"PresentacionView\",\n  data() {\n    return {\n      presentaciones: [],\n      form: {\n        id: null,\n        nombre: \"\",\n        descripcion: \"\"\n      },\n      editando: false,\n      page: 1,\n      per_page: 10,\n      search: \"\",\n      loading: false,\n      searchTimer: null\n    };\n  },\n  mounted() {\n    this.cargarPresentaciones();\n  },\n  methods: {\n    async cargarPresentaciones() {\n      this.loading = true;\n      try {\n        const params = {\n          page: this.page,\n          per_page: this.per_page,\n          search: this.search\n        };\n        const {\n          data\n        } = await http.get(\"/presentaciones/\", {\n          params\n        });\n        // backend devuelve { total, page, per_page, data: [] }\n        this.presentaciones = Array.isArray(data?.data) ? data.data : Array.isArray(data) ? data : [];\n      } catch (e) {\n        console.error(e);\n        Swal.fire(\"Error\", e?.response?.data?.error || \"Error al cargar presentaciones\", \"error\");\n      } finally {\n        this.loading = false;\n      }\n    },\n    // debounce simple de búsqueda\n    onSearchInput() {\n      clearTimeout(this.searchTimer);\n      this.searchTimer = setTimeout(() => {\n        this.page = 1;\n        this.cargarPresentaciones();\n      }, 350);\n    },\n    async confirmarGuardar() {\n      const result = await Swal.fire({\n        title: \"¿Está seguro que desea guardar la información ingresada?\",\n        icon: \"question\",\n        showCancelButton: true,\n        confirmButtonText: \"Sí, guardar\",\n        cancelButtonText: \"Cancelar\"\n      });\n      if (result.isConfirmed) this.guardarPresentacion();\n    },\n    async guardarPresentacion() {\n      if (!this.form.nombre?.trim()) {\n        Swal.fire(\"Validación\", \"El nombre es requerido\", \"warning\");\n        return;\n      }\n      this.loading = true;\n      try {\n        const url = this.editando ? `/presentaciones/${this.form.id}` : `/presentaciones/`;\n        const method = this.editando ? \"put\" : \"post\";\n        const payload = {\n          nombre: this.form.nombre?.trim(),\n          descripcion: this.form.descripcion?.trim() || null\n        };\n        const {\n          status,\n          data\n        } = await http[method](url, payload);\n        if (!String(status).startsWith(\"2\")) {\n          throw new Error(data?.error || \"Error al guardar la presentación\");\n        }\n        this.resetForm();\n        this.editando = false;\n        await this.cargarPresentaciones();\n        Swal.fire(\"Éxito\", \"Presentación guardada correctamente\", \"success\");\n      } catch (e) {\n        console.error(e);\n        Swal.fire(\"Error\", e?.response?.data?.error || \"Error al guardar la presentación\", \"error\");\n      } finally {\n        this.loading = false;\n      }\n    },\n    async editarPresentacion(presentacion) {\n      const result = await Swal.fire({\n        title: \"¿Editar presentación?\",\n        text: `¿Deseas editar la presentación \"${presentacion.nombre}\"?`,\n        icon: \"question\",\n        showCancelButton: true,\n        confirmButtonText: \"Sí, editar\",\n        cancelButtonText: \"Cancelar\"\n      });\n      if (result.isConfirmed) {\n        this.form = {\n          id: presentacion.id,\n          nombre: presentacion.nombre,\n          descripcion: presentacion.descripcion || \"\"\n        };\n        this.editando = true;\n        window.scrollTo({\n          top: 0,\n          behavior: \"smooth\"\n        });\n      }\n    },\n    cancelarEdicion() {\n      this.resetForm();\n      this.editando = false;\n    },\n    async confirmarEliminar(id) {\n      const result = await Swal.fire({\n        title: \"¿Estás seguro?\",\n        text: \"No podrás revertir esta acción\",\n        icon: \"warning\",\n        showCancelButton: true,\n        confirmButtonColor: \"#3085d6\",\n        cancelButtonColor: \"#d33\",\n        confirmButtonText: \"Sí, eliminar\",\n        cancelButtonText: \"Cancelar\"\n      });\n      if (result.isConfirmed) {\n        this.eliminarPresentacion(id);\n      }\n    },\n    async eliminarPresentacion(id) {\n      this.loading = true;\n      try {\n        const {\n          status,\n          data\n        } = await http.delete(`/presentaciones/${id}`);\n        if (!String(status).startsWith(\"2\")) {\n          throw new Error(data?.error || \"No se pudo eliminar\");\n        }\n        Swal.fire(\"Eliminado\", \"La presentación ha sido eliminada\", \"success\");\n        if (this.presentaciones.length === 1 && this.page > 1) this.page -= 1;\n        await this.cargarPresentaciones();\n      } catch (e) {\n        console.error(e);\n        Swal.fire(\"Error\", e?.response?.data?.error || \"Error de red al eliminar\", \"error\");\n      } finally {\n        this.loading = false;\n      }\n    },\n    resetForm() {\n      this.form = {\n        id: null,\n        nombre: \"\",\n        descripcion: \"\"\n      };\n    },\n    cambiarPagina(nuevaPagina) {\n      if (nuevaPagina < 1 || this.loading) return;\n      this.page = nuevaPagina;\n      this.cargarPresentaciones();\n    }\n  }\n};","map":{"version":3,"names":["Swal","http","name","data","presentaciones","form","id","nombre","descripcion","editando","page","per_page","search","loading","searchTimer","mounted","cargarPresentaciones","methods","params","get","Array","isArray","e","console","error","fire","response","onSearchInput","clearTimeout","setTimeout","confirmarGuardar","result","title","icon","showCancelButton","confirmButtonText","cancelButtonText","isConfirmed","guardarPresentacion","trim","url","method","payload","status","String","startsWith","Error","resetForm","editarPresentacion","presentacion","text","window","scrollTo","top","behavior","cancelarEdicion","confirmarEliminar","confirmButtonColor","cancelButtonColor","eliminarPresentacion","delete","length","cambiarPagina","nuevaPagina"],"sources":["C:\\Users\\pedvi\\Documents\\catalogo-web\\frontend\\src\\views\\PresentacionView.vue"],"sourcesContent":["<template>\r\n  <div class=\"container mt-4\">\r\n    <h2>Presentaciones</h2>\r\n\r\n    <!-- Formulario Crear/Editar -->\r\n    <form @submit.prevent=\"confirmarGuardar\" class=\"mb-4\">\r\n      <div class=\"mb-3\">\r\n        <label class=\"form-label\">Nombre</label>\r\n        <input v-model.trim=\"form.nombre\" type=\"text\" class=\"form-control\" required />\r\n      </div>\r\n      <div class=\"mb-3\">\r\n        <label class=\"form-label\">Descripción</label>\r\n        <input v-model.trim=\"form.descripcion\" type=\"text\" class=\"form-control\" />\r\n      </div>\r\n      <button :disabled=\"loading\" type=\"submit\" class=\"btn btn-primary\">\r\n        {{ editando ? 'Actualizar' : 'Agregar' }} Presentación\r\n      </button>\r\n      <button\r\n        v-if=\"editando\"\r\n        :disabled=\"loading\"\r\n        type=\"button\"\r\n        class=\"btn btn-secondary ms-2\"\r\n        @click=\"cancelarEdicion\"\r\n      >\r\n        Cancelar\r\n      </button>\r\n    </form>\r\n\r\n    <!-- Filtro de búsqueda -->\r\n    <div class=\"mb-3\">\r\n      <input\r\n        v-model=\"search\"\r\n        type=\"text\"\r\n        class=\"form-control\"\r\n        placeholder=\"Buscar por nombre...\"\r\n        @input=\"onSearchInput\"\r\n      />\r\n    </div>\r\n\r\n    <!-- Tabla de Presentaciones -->\r\n    <table class=\"table table-bordered\">\r\n      <thead>\r\n        <tr>\r\n          <th style=\"width: 80px;\">ID</th>\r\n          <th>Nombre</th>\r\n          <th>Descripción</th>\r\n          <th style=\"width: 180px;\">Acciones</th>\r\n        </tr>\r\n      </thead>\r\n      <tbody>\r\n        <tr v-for=\"presentacion in presentaciones\" :key=\"presentacion.id\">\r\n          <td>{{ presentacion.id }}</td>\r\n          <td>{{ presentacion.nombre }}</td>\r\n          <td>{{ presentacion.descripcion }}</td>\r\n          <td>\r\n            <button class=\"btn btn-sm btn-warning me-2\" @click=\"editarPresentacion(presentacion)\">Editar</button>\r\n            <button class=\"btn btn-sm btn-danger\" @click=\"confirmarEliminar(presentacion.id)\">Eliminar</button>\r\n          </td>\r\n        </tr>\r\n        <tr v-if=\"!loading && presentaciones.length === 0\">\r\n          <td colspan=\"4\" class=\"text-center\">Sin resultados</td>\r\n        </tr>\r\n        <tr v-if=\"loading\">\r\n          <td colspan=\"4\" class=\"text-center\">Cargando...</td>\r\n        </tr>\r\n      </tbody>\r\n    </table>\r\n\r\n    <!-- Paginación simple -->\r\n    <nav>\r\n      <ul class=\"pagination\">\r\n        <li class=\"page-item\" :class=\"{ disabled: page === 1 || loading }\">\r\n          <button class=\"page-link\" @click=\"cambiarPagina(page - 1)\" :disabled=\"page === 1 || loading\">\r\n            Anterior\r\n          </button>\r\n        </li>\r\n        <li class=\"page-item\">\r\n          <span class=\"page-link\">{{ page }}</span>\r\n        </li>\r\n        <li class=\"page-item\">\r\n          <button class=\"page-link\" @click=\"cambiarPagina(page + 1)\" :disabled=\"loading\">\r\n            Siguiente\r\n          </button>\r\n        </li>\r\n      </ul>\r\n    </nav>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Swal from \"sweetalert2\";\r\nimport http from \"@/api/http\"; // axios con interceptores (access + refresh)\r\n\r\nexport default {\r\n  name: \"PresentacionView\",\r\n  data() {\r\n    return {\r\n      presentaciones: [],\r\n      form: { id: null, nombre: \"\", descripcion: \"\" },\r\n      editando: false,\r\n      page: 1,\r\n      per_page: 10,\r\n      search: \"\",\r\n      loading: false,\r\n      searchTimer: null,\r\n    };\r\n  },\r\n  mounted() {\r\n    this.cargarPresentaciones();\r\n  },\r\n  methods: {\r\n    async cargarPresentaciones() {\r\n      this.loading = true;\r\n      try {\r\n        const params = { page: this.page, per_page: this.per_page, search: this.search };\r\n        const { data } = await http.get(\"/presentaciones/\", { params });\r\n        // backend devuelve { total, page, per_page, data: [] }\r\n        this.presentaciones = Array.isArray(data?.data) ? data.data : (Array.isArray(data) ? data : []);\r\n      } catch (e) {\r\n        console.error(e);\r\n        Swal.fire(\"Error\", e?.response?.data?.error || \"Error al cargar presentaciones\", \"error\");\r\n      } finally {\r\n        this.loading = false;\r\n      }\r\n    },\r\n\r\n    // debounce simple de búsqueda\r\n    onSearchInput() {\r\n      clearTimeout(this.searchTimer);\r\n      this.searchTimer = setTimeout(() => {\r\n        this.page = 1;\r\n        this.cargarPresentaciones();\r\n      }, 350);\r\n    },\r\n\r\n    async confirmarGuardar() {\r\n      const result = await Swal.fire({\r\n        title: \"¿Está seguro que desea guardar la información ingresada?\",\r\n        icon: \"question\",\r\n        showCancelButton: true,\r\n        confirmButtonText: \"Sí, guardar\",\r\n        cancelButtonText: \"Cancelar\",\r\n      });\r\n      if (result.isConfirmed) this.guardarPresentacion();\r\n    },\r\n\r\n    async guardarPresentacion() {\r\n      if (!this.form.nombre?.trim()) {\r\n        Swal.fire(\"Validación\", \"El nombre es requerido\", \"warning\");\r\n        return;\r\n      }\r\n\r\n      this.loading = true;\r\n      try {\r\n        const url = this.editando ? `/presentaciones/${this.form.id}` : `/presentaciones/`;\r\n        const method = this.editando ? \"put\" : \"post\";\r\n        const payload = {\r\n          nombre: this.form.nombre?.trim(),\r\n          descripcion: this.form.descripcion?.trim() || null,\r\n        };\r\n\r\n        const { status, data } = await http[method](url, payload);\r\n        if (!String(status).startsWith(\"2\")) {\r\n          throw new Error(data?.error || \"Error al guardar la presentación\");\r\n        }\r\n\r\n        this.resetForm();\r\n        this.editando = false;\r\n        await this.cargarPresentaciones();\r\n        Swal.fire(\"Éxito\", \"Presentación guardada correctamente\", \"success\");\r\n      } catch (e) {\r\n        console.error(e);\r\n        Swal.fire(\"Error\", e?.response?.data?.error || \"Error al guardar la presentación\", \"error\");\r\n      } finally {\r\n        this.loading = false;\r\n      }\r\n    },\r\n\r\n    async editarPresentacion(presentacion) {\r\n      const result = await Swal.fire({\r\n        title: \"¿Editar presentación?\",\r\n        text: `¿Deseas editar la presentación \"${presentacion.nombre}\"?`,\r\n        icon: \"question\",\r\n        showCancelButton: true,\r\n        confirmButtonText: \"Sí, editar\",\r\n        cancelButtonText: \"Cancelar\",\r\n      });\r\n\r\n      if (result.isConfirmed) {\r\n        this.form = {\r\n          id: presentacion.id,\r\n          nombre: presentacion.nombre,\r\n          descripcion: presentacion.descripcion || \"\",\r\n        };\r\n        this.editando = true;\r\n        window.scrollTo({ top: 0, behavior: \"smooth\" });\r\n      }\r\n    },\r\n\r\n    cancelarEdicion() {\r\n      this.resetForm();\r\n      this.editando = false;\r\n    },\r\n\r\n    async confirmarEliminar(id) {\r\n      const result = await Swal.fire({\r\n        title: \"¿Estás seguro?\",\r\n        text: \"No podrás revertir esta acción\",\r\n        icon: \"warning\",\r\n        showCancelButton: true,\r\n        confirmButtonColor: \"#3085d6\",\r\n        cancelButtonColor: \"#d33\",\r\n        confirmButtonText: \"Sí, eliminar\",\r\n        cancelButtonText: \"Cancelar\",\r\n      });\r\n\r\n      if (result.isConfirmed) {\r\n        this.eliminarPresentacion(id);\r\n      }\r\n    },\r\n\r\n    async eliminarPresentacion(id) {\r\n      this.loading = true;\r\n      try {\r\n        const { status, data } = await http.delete(`/presentaciones/